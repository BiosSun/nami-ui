{
    "name": "nami-ui",
    "version": "0.0.0",
    "description": "Nami UI 是一套基于 React 的工具集及组件库",
    "repository": "ssh://git@gitlab.biossun.xyz:10022/nami-ui/nami.git",
    "author": "Bios Sun <biossun@gmail.com>",
    "license": "MIT",
    "private": true,
    "workspaces": {
        "packages": [
            "packages/*"
        ]
    },
    "scripts": {
        "bootstrap": "yarn install",
        "clean": "node scripts/clean.js",
        "website": "yarn workspace @nami-ui/website run start",
        "storybook": "start-storybook -p 6006",
        "release": "lerna version --no-push --force-git-tag && node scripts/prepublish.js && lerna publish from-git --contents dist && node scripts/postpublish.js",
        "release:website": "yarn workspace @nami-ui/website run build && gh-pages -d packages/website/build/ -t",
        "yalc": "node scripts/yalc.js"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@k-vyn/coloralgorithm": "^1.0.0",
        "@storybook/addon-actions": "^6.2.3",
        "@storybook/addon-essentials": "^6.2.3",
        "@storybook/addon-links": "^6.2.3",
        "@storybook/builder-webpack5": "^6.2.3",
        "@storybook/preset-scss": "^1.0.3",
        "@storybook/react": "^6.2.3",
        "@types/jest": "^26.0.22",
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "babel-loader": "^8.2.2",
        "chalk": "^4.1.0",
        "css-loader": "^5.2.0",
        "del": "^6.0.0",
        "eslint": "^7.23.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.23.1",
        "eslint-plugin-react-hooks": "4.2.0",
        "gh-pages": "^3.1.0",
        "globby": "^11.0.3",
        "gulp": "^4.0.2",
        "gulp-if": "^3.0.0",
        "gulp-json-editor": "^2.5.5",
        "gulp-replace": "^1.0.0",
        "gulp-sass": "^4.1.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "jest": "^26.6.3",
        "lerna": "^4.0.0",
        "merge2": "^1.4.1",
        "prettier": "^2.2.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-is": "^17.0.2",
        "react-jss": "^10.6.0",
        "sass": "^1.32.8",
        "sass-loader": "^10.1.1",
        "style-loader": "^2.0.0",
        "toposort": "^2.0.2",
        "ts-jest": "^26.5.4",
        "typescript": "^4.2.3"
    },
    "resolutions": {
        "@babel/runtime": "7.12.13",
        "terser": "4.8.0"
    }
}
